AGA,KAAA,YAAA,CAAA,MAAA,EAAA,sBAAA,EAAA,KAAA,EAAA;AAAA,UAAA,WAAA,EAAA,KAAA;AAAA,UAAA,IAAA,EAAA;AAAA,SAAA,CAHA,sBAIA,KAAA,YAAA,CAAA,MAAA,EAAA,YAAA,EAAA,KAAA,EAAA;AAAA,UAAA,WAAA,EAAA;AAAA,SAAA,CAJA;AAHA,SASA,KAAA,SAAA,8BACA,KAAA,qBAAA,CAAA,YAAA,EAAA,EAAA,CADA,sBAEA,KAAA,qBAAA,CAAA,YAAA,EAAA,EAAA,CAFA,GATA;AAZA,KAAA;AA2BA,GAxCA;AAyCA,EAAA,OAzCA,qBAyCA;AACA,QAAA,KAAA,OAAA,IAAA,KAAA,OAAA,CAAA,YAAA,EAAA;AACA,WAAA,MAAA,CAAA,IAAA,GAAA,KAAA,OAAA,CAAA,YAAA;AACA;AACA,GA7CA;AA8CA,EAAA,OAAA,EAAA;AACA;AACA,IAAA,SAFA,qBAEA,IAFA,EAEA;AACA,WAAA,IAAA,GAAA,IAAA;AACA,WAAA,IAAA,CAAA,KAAA,GAAA,IAAA,CAAA,MAAA;AACA,WAAA,IAAA,CAAA,QAAA,GAAA,KAAA,IAAA,CAAA,KAAA;AACA,KANA;AAOA;AACA,IAAA,UARA,wBAQA;AACA,WAAA,MAAA,CAAA,KAAA,GAAA,KAAA,GAAA,CAAA,KAAA;AACA,WAAA,MAAA,CAAA,IAAA,GAAA,KAAA,CAAA,KAAA,GAAA,CAAA,cAAA,CAAA,CAAA,MAAA,CAAA,SAAA,CAAA;AACA;AAXA;AA9CA,CAAA","sourcesContent":["<template>\n    <div class=\"app-container\">\n        <avue-crud v-bind=\"bindVal\" v-on=\"onEvent\" v-model=\"form\" :page.sync=\"page\">\n            <template slot-scope=\"scope\" slot=\"expressFee\">\n                <span v-if=\"scope.row.expressFee&&scope.row.expressFee>0\" @click=\"rowView({ ...scope.row, formslot:'expressDay',viewTitle:`商家[${row.companyName}](${scope.row.day})的快递详情`},scope.index)\"\n                    style=\"color: rgb(190, 149, 74);cursor: pointer;\">{{scope.row.expressFee|filtersFormatMoney}}</span>\n                <span v-else>--</span>\n            </template>\n            <template slot-scope=\"scope\" slot=\"qualityInspectionFee\">\n                <span v-if=\"scope.row.qualityInspectionFee&&scope.row.qualityInspectionFee>0\"\n                    @click=\"rowView({ ...scope.row, formslot:'qualityDay' ,viewTitle:`商家[${row.companyName}](${scope.row.day})的质检详情`},scope.index)\"\n                    style=\"color: rgb(190, 149, 74);cursor: pointer;\">{{scope.row.qualityInspectionFee|filtersFormatMoney}}</span>\n                <span v-else>--</span>\n            </template>\n            <div slot=\"qualityDayForm\" slot-scope=\"scope\">\n                <QualityDay :Pconfig=\"Pconfig\" :row=\"{...scope.row,...row}\" v-if=\"form&&form.formslot==scope.column.prop\"></QualityDay>\n            </div>\n            <div slot=\"expressDayForm\" slot-scope=\"scope\">\n                <ExpressDay :Pconfig=\"Pconfig\" :row=\"{...scope.row,...row}\" v-if=\"form&&form.formslot==scope.column.prop\"></ExpressDay>\n            </div>\n        </avue-crud>\n    </div>\n</template>\n<script>\n/**\n * 混入\n */\nimport crudMix from \"@/mixins/crudMix\";\n/**\n * 组件\n */\nimport QualityDay from '@/views/company/list/UsageRecord/QualityDay'\nimport ExpressDay from '@/views/company/list/UsageRecord/ExpressDay'\n/**\n * TODO:每日详情\n */\nexport default {\n  components: {\n    QualityDay,\n    ExpressDay\n  },\n  mixins: [\n    crudMix,\n  ],\n  props: {\n    row: {},\n    Pconfig: {},\n  },\n  data() {\n    return {\n      config: {\n        detail: '',\n        save: '',\n        delete: '',\n        update: '',\n        list: '/company/findDayOrder'\n      },\n      method: {\n        list: 'GET'\n      },\n      rowKey: 'bicId',\n      option: {\n        \n        menu: false,\n        column: [\n          ...this.column_def(\"使用时间\", \"day\", false, {}),\n          ...this.column_money(\"快递费用\", \"expressFee\", false, { viewDisplay: false, solt: true, }),\n          ...this.column_money(\"质检费用\", \"qualityInspectionFee\", false, { viewDisplay: false, solt: true, }),\n          ...this.column_money(\"使用金额\", \"monthMoney\", false, { viewDisplay: false })\n        ],\n        ...this.group_def([\n          ...this.group_column_formslot(\"qualityDay\", {}),\n          ...this.group_column_formslot(\"expressDay\", {}),\n        ]),\n      },\n    }\n  },\n  created() {\n     if (this.Pconfig && this.Pconfig.findDayOrder) {\n      this.config.list = this.Pconfig.findDayOrder;\n    }\n  },\n  methods: {\n    //获取列表后\n    listAfter(data) {\n      this.data = data;\n      this.page.total = data.length;\n      this.page.pageSize = this.page.total;\n    },\n    //获取列表前\n    listBefore() {\n      this.params.bicId = this.row.bicId\n      this.params.time = dayjs(this.row.statisticsTime).format('YYYY-MM')\n    },\n  }\n}\n</script>\n"],"sourceRoot":"src/views/company/list/UsageRecord"}]}