AAA;AACA,OAHA;AAIA,KANA;AAOA,IAAA,iBAPA,+BAOA;AAAA;;AACA,MAAA,cAAA,CAAA,YAAA,CAAA,CACA,IADA,CACA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA,EAAA,KAAA;;AACA,YAAA,GAAA,CAAA,SAAA,KAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,eAAA,GAAA,GAAA,CAAA,IAAA;AACA,UAAA,MAAA,CAAA,eAAA,GAAA,GAAA,CAAA,IAAA;AACA;AACA,OAPA;AAQA,KAhBA;AAiBA,IAAA,MAjBA,oBAiBA;AACA,WAAA,OAAA,CAAA,EAAA,CAAA,CAAA,CAAA;AACA,KAnBA;AAoBA,IAAA,OApBA,mBAoBA,MApBA,EAoBA;AAAA;;AACA,MAAA,cAAA,CAAA,MAAA,CAAA,CACA,IADA,CACA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,SAAA,KAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,IAAA,GAAA,GAAA,CAAA,IAAA;AACA;AACA,OALA;AAMA,KA3BA;AA4BA,IAAA,eA5BA,2BA4BA,IA5BA,EA4BA,EA5BA,EA4BA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,IAAA,EAAA,EAAA;AACA,KA9BA;AA+BA,IAAA,mBA/BA,+BA+BA,GA/BA,EA+BA;AACA,WAAA,MAAA,CAAA,IAAA,GAAA,GAAA;AACA,WAAA,OAAA,CAAA,KAAA,MAAA;AACA,KAlCA;AAmCA,IAAA,gBAnCA,4BAmCA,GAnCA,EAmCA;AACA,WAAA,MAAA,CAAA,IAAA,GAAA,GAAA;AACA,WAAA,OAAA,CAAA,KAAA,MAAA;AACA;AAtCA;AAtBA,CAAA","sourcesContent":["<template>\n  <div>\n    <basic-container-back>\n      <span class=\"tip-info\"></span>\n      <span class=\"tip-title\">基本信息</span>\n      <div style=\"margin-top: 20px\">\n        <el-row :gutter=\"4\" class=\"m-bottom\">\n          <el-col :span=\"8\">\n            <span class=\"student-title\">学生ID：</span>\n            <span class=\"student-info\">{{ info._id }}</span>\n          </el-col>\n          <el-col :span=\"8\">\n            <div style=\"display: flex;align-items: center\">\n              <span class=\"student-title\">头像：</span>\n              <el-avatar size=\"30\" :src=\"info.profilePicture\" style=\"margin-top: -20px\" />\n            </div>\n          </el-col>\n          <el-col :span=\"8\">\n            <span class=\"student-title\">姓名：</span>\n            <span class=\"student-info\">{{ info.username }}</span>\n          </el-col>\n        </el-row>\n        <el-row :gutter=\"4\">\n          <el-col :span=\"8\">\n            <span class=\"student-title\">性别：</span>\n            <span class=\"student-info\">{{ info.gender == 'f' ? '男' : '女' }}</span>\n          </el-col>\n          <el-col :span=\"8\">\n            <span class=\"student-title\">手机号：</span>\n            <span class=\"student-info\">{{info.username}}</span>\n          </el-col>\n          <el-col :span=\"8\">\n            <span class=\"student-title\">激活日期：</span>\n            <span class=\"student-info\">{{ info.activationDate }}</span>\n          </el-col>\n        </el-row>\n      </div>\n    </basic-container-back>\n    <basic-container>\n      <span class=\"tip-info\"></span>\n      <span class=\"tip-title\">班级信息</span>\n      <div style=\"margin-top: 20px\" v-if=\"info.gradeDto\">\n        <el-row :gutter=\"4\" class=\"m-bottom\">\n          <el-col :span=\"8\">\n            <span class=\"student-title\">班级ID：</span>\n            <span class=\"student-info\">{{ info.gradeDto._id }}</span>\n          </el-col>\n          <el-col :span=\"8\">\n            <span class=\"student-title\">班级名称：</span>\n            <span class=\"student-info\">{{info.gradeDto.name}}</span>\n          </el-col>\n          <el-col :span=\"8\">\n            <span class=\"student-title\">班级类型：</span>\n            <span class=\"student-info\">{{ info.gradeDto.gradeType }}</span>\n          </el-col>\n        </el-row>\n        <el-row :gutter=\"4\" class=\"m-bottom\">\n          <el-col :span=\"8\">\n            <span class=\"student-title\">年级：</span>\n            <span class=\"student-info\">{{ info.gradeDto.grade }}</span>\n          </el-col>\n          <el-col :span=\"8\">\n            <span class=\"student-title\">入学年份：</span>\n            <span class=\"student-info\">{{info.gradeDto.enrollmentYear}}</span>\n          </el-col>\n          <el-col :span=\"8\">\n            <span class=\"student-title\">班主任：</span>\n            <span class=\"student-info\">{{ info.gradeDto.teacherName }}</span>\n          </el-col>\n        </el-row>\n        <el-row :gutter=\"4\" class=\"m-bottom\">\n          <el-col :span=\"8\">\n            <span class=\"student-title\">生涯导师：</span>\n            <span class=\"student-info\">{{ info.gradeDto.teacher1Name }}</span>\n          </el-col>\n          <el-col :span=\"8\">\n            <span class=\"student-title\">生涯导师2：</span>\n            <span class=\"student-info\">{{info.gradeDto.teacher2Name}}</span>\n          </el-col>\n        </el-row>\n      </div>\n      <basic-nothing v-else title=\"该学生未绑定任何班级\"></basic-nothing>\n    </basic-container>\n\n    <basic-container style=\"margin-bottom: 100px\">\n      <span class=\"tip-info\"></span>\n      <span class=\"tip-title\">选科信息</span>\n      <div style=\"margin-top: 20px\" v-if=\"info.firstChoice\">\n        <el-row :gutter=\"4\">\n         <el-col :span=\"8\">\n           <span class=\"student-title\">首选科目: </span>\n           <span class=\"student-info\">{{info.firstChoice}}</span>\n         </el-col>\n          <el-col :span=\"8\">\n            <span class=\"student-title\">再选科目: </span>\n            <span class=\"student-info\">{{info.recleaning1}}、{{info.recleaning2}}</span>\n            </el-col>\n        </el-row>\n\n<!--        <span class=\"tip-title\">首选科目</span>-->\n<!--        <el-select v-model=\"info.firstChoice\" placeholder=\"请选择\" clearable class=\"m-right\" disabled>-->\n<!--          <el-option v-for=\"(item, index) in firstList\" :key=\"index\" :label=\"item.name\" :value=\"item.name\"/>-->\n<!--        </el-select>-->\n<!--        <span class=\"tip-title\">再选科目1</span>-->\n<!--        <el-select v-model=\"info.recleaning1\" placeholder=\"请选择\" clearable class=\"m-right\" disabled>-->\n<!--          <el-option v-for=\"(item, index) in recleaning1List\" :key=\"index\" :label=\"item.name\" :value=\"item.name\"/>-->\n<!--        </el-select>-->\n<!--        <span class=\"tip-title\">再选科目2</span>-->\n<!--        <el-select v-model=\"info.recleaning2\" placeholder=\"请选择\" clearable class=\"m-right\" disabled>-->\n<!--          <el-option v-for=\"(item, index) in recleaning2List\" :key=\"index\" :label=\"item.name\" :value=\"item.name\"/>-->\n<!--        </el-select>-->\n      </div>\n      <basic-nothing v-else title=\"该学生未填写选科信息\"></basic-nothing>\n    </basic-container>\n    <div class=\"footer-btn\">\n      <el-button style=\"color: #475B75\" @click=\"goBack\">返回</el-button>\n    </div>\n  </div>\n</template>\n\n<script>\nimport {getStudentInfo} from \"../../../api/students\";\nimport {selectClassList, selectTypeList} from \"../../../api/search\";\n\nexport default {\n  name: \"visit\",\n  data() {\n    return {\n      params: {\n        studentId: '',\n        gradeId: ''\n      },\n      info: {},\n      firstList: [],\n      recleaning1List: [],\n      recleaning2List: []\n    }\n  },\n  mounted() {\n    let {studentId, gradeId} = this.$route.query;\n    this.params.studentId = studentId;\n    this.params.gradeId = gradeId;\n    this.getInfo(this.params);\n    this.getFirstSelectData();\n    this.getRecleaningData();\n  },\n  methods: {\n    getFirstSelectData() {\n      selectTypeList('firstChoice')\n          .then(res => {\n            if (res.errorCode === 200) this.firstList = res.data;\n          })\n    },\n    getRecleaningData() {\n      selectTypeList('recleaning')\n          .then(res => {\n            console.log(res, 'sad');\n            if (res.errorCode === 200) {\n              this.recleaning1List = res.data;\n              this.recleaning2List = res.data;\n            }\n          })\n    },\n    goBack() {\n      this.$router.go(-1);\n    },\n    getInfo(params) {\n      getStudentInfo(params)\n          .then(res => {\n            if (res.errorCode === 200) {\n              this.info = res.data;\n            }\n          })\n    },\n    goOperationType(type, id) {\n      console.log(type, id);\n    },\n    handleCurrentChange(val) {\n      this.params.page = val;\n      this.getInfo(this.params);\n    },\n    handleSizeChange(val) {\n      this.params.size = val;\n      this.getInfo(this.params);\n    }\n  }\n}\n</script>\n\n<style scoped lang=\"scss\">\n.student-title {\n  height: 22px;\n  font-size: 14px;\n  font-family: PingFangSC-Regular, PingFang SC;\n  font-weight: 400;\n  color: #2E415B;\n  line-height: 22px;\n  margin-right: 20px;\n  margin-bottom: 20px;\n}\n\n.student-info {\n  height: 22px;\n  font-size: 14px;\n  font-family: PingFangSC-Regular, PingFang SC;\n  font-weight: 400;\n  color: #506889;\n  line-height: 22px;\n}\n\n\n.m-right {\n  margin-right: 20px;\n}\n</style>"],"sourceRoot":"src/views/students/operation"}]}