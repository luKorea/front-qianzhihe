AEA,YAAA,EAAA,EAAA;AAFA;AAFA,SAAA;AAOA;AACA,KAhBA;AAiBA,IAAA,UAjBA,sBAiBA,EAjBA,EAiBA;AAAA;;AACA,WAAA,QAAA,CAAA,oBAAA,EAAA,MAAA,EAAA;AACA,QAAA,iBAAA,EAAA,IADA;AAEA,QAAA,gBAAA,EAAA,IAFA;AAGA,QAAA,IAAA,EAAA;AAHA,OAAA,EAIA,IAJA,CAIA,YAAA;AACA,QAAA,KAAA,CAAA,QAAA,CAAA;AACA,UAAA,IAAA,EAAA,SADA;AAEA,UAAA,OAAA,EAAA;AAFA,SAAA;AAIA,OATA,EASA,KATA,CASA,YAAA,CACA,CAVA;AAWA,KA7BA;AA8BA,IAAA,cA9BA,4BA8BA;AAAA;;AACA,MAAA,cAAA,CAAA,aAAA,CAAA,CACA,IADA,CACA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,SAAA,KAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,WAAA,GAAA,GAAA,CAAA,IAAA;AACA;AACA,OALA;AAMA,KArCA;AAsCA,IAAA,YAtCA,0BAsCA;AAAA;;AACA,MAAA,cAAA,CAAA,OAAA,CAAA,CACA,IADA,CACA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,SAAA,KAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,SAAA,GAAA,GAAA,CAAA,IAAA;AACA;AACA,OALA;AAMA,KA7CA;AA8CA,IAAA,OA9CA,mBA8CA,MA9CA,EA8CA;AAAA;;AACA,MAAA,cAAA,CAAA,MAAA,CAAA,CACA,IADA,CACA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,SAAA,KAAA,GAAA,EAAA;AACA,cAAA,IAAA,GAAA,GAAA,CAAA,IAAA;AACA,UAAA,MAAA,CAAA,IAAA,GAAA,IAAA,CAAA,MAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,UAAA,MAAA,CAAA,MAAA,CAAA,KAAA,GAAA,IAAA,CAAA,UAAA,CAAA,KAAA,IAAA,CAAA;AACA;AACA,OARA;AASA,KAxDA;AAyDA,IAAA,mBAzDA,+BAyDA,GAzDA,EAyDA;AACA,WAAA,MAAA,CAAA,IAAA,GAAA,GAAA;AACA,WAAA,OAAA,CAAA,KAAA,MAAA;AACA,KA5DA;AA6DA,IAAA,gBA7DA,4BA6DA,GA7DA,EA6DA;AACA,WAAA,MAAA,CAAA,IAAA,GAAA,GAAA;AACA,WAAA,OAAA,CAAA,KAAA,MAAA;AACA;AAhEA;AAtBA,CAAA","sourcesContent":["<template>\n  <div>\n    <basic-container>\n      <span class=\"tip-info\"></span>\n      <span class=\"tip-title\">教师筛选</span>\n      <div style=\"margin-top: 20px\">\n        <el-row >\n         <el-col :span=\"6\">\n           <span class=\"tip\">教师类型:</span>\n           <el-select v-model=\"params.teacherType\" placeholder=\"请选择\" clearable>\n             <template v-if=\"teacherList && teacherList.length > 0\">\n               <el-option v-for=\"item in teacherList\" :label=\"item.name\" :value=\"item.name\"></el-option>\n             </template>\n           </el-select>\n         </el-col>\n          <el-col :span=\"6\">\n            <span class=\"tip\">代表年级:</span>\n            <el-select v-model=\"params.grade\" placeholder=\"请选择\" clearable>\n              <template v-if=\"gradeList && gradeList.length > 0\">\n                <el-option v-for=\"item in gradeList\" :label=\"item.name\" :value=\"item.name\"></el-option>\n              </template>\n            </el-select>\n          </el-col>\n          <el-col :span='8'>\n            <div style=\"display:flex;\">\n              <el-input style=\"margin-right: 10px\" v-model=\"params.queryOrTeacherNameOrPhone\" placeholder=\"请输入教师名称、手机号\" clearable=\"true\"/>\n              <el-button type=\"primary\" @click=\"getData(params)\">筛选</el-button>\n            </div>\n          </el-col>\n        </el-row>\n      </div>\n    </basic-container>\n    <basic-container>\n      <div style=\"display: flex; justify-content: space-between;align-items: center;margin-bottom: 10px\">\n        <div>\n          <span class=\"tip-info\"></span>\n          <span class=\"tip-title\">教师列表</span>\n        </div>\n        <el-button type=\"primary\" @click=\"goOperationType('add')\">添加老师</el-button>\n      </div>\n      <el-table :data=\"list\" border style=\"width: 100%;margin-bottom: 20px\">\n        <el-table-column prop=\"_id\" label=\"教师ID\" align=\"center\" width=\"300\" />\n        <el-table-column prop=\"name\" label=\"教师名称\" align=\"center\" />\n        <el-table-column prop=\"teacherType\" label=\"教师类型\" align=\"center\" />\n        <el-table-column prop=\"phone\" label=\"手机号\" align=\"center\" />\n        <el-table-column label=\"代课班级\" align=\"center\">\n          <template slot-scope=\"scope\" v-if=\"scope.row.teacherGradeDtoList && scope.row.teacherGradeDtoList.length > 0\">\n            <span v-for=\"item in scope.row.teacherGradeDtoList\" :key=\"item._id\">{{item.name}}；</span>\n          </template>\n        </el-table-column>\n        <el-table-column label=\"操作\" align=\"center\">\n          <template slot-scope=\"scope\">\n            <el-button type=\"text\" size=\"small\" @click=\"goOperationType('visit', scope.row._id)\">查看</el-button>\n            <el-button type=\"text\" size=\"small\" @click=\"goOperationType('edit', scope.row._id)\">编辑</el-button>\n            <el-button type=\"text\" size=\"small\" style=\"color: red\" @click=\"deleteData(scope.row._id)\">删除</el-button>\n          </template>\n        </el-table-column>\n      </el-table>\n      <basic-pagination\n          :total=\"params.total\"\n          @handleCurrentChange=\"handleCurrentChange\"\n          @handleSizeChange=\"handleSizeChange\"\n      />\n    </basic-container>\n  </div>\n</template>\n\n<script>\nimport {getTeacherList} from \"../../api/taecher\";\nimport {selectTypeList} from \"../../api/search\";\n\nexport default {\n  name: \"teacherList\",\n  data() {\n    return {\n      params: {\n        page: 0,\n        size: 10,\n        queryOrTeacherNameOrPhone: '',\n        grade: '',\n        teacherType: '',\n        total: 0\n      },\n      teacherList: [],\n      gradeList: [],\n      list: []\n    }\n  },\n  mounted() {\n    this.getData(this.params);\n    this.getGradeType();\n    this.getTeacherType();\n  },\n  methods: {\n    goOperationType(type, id) {\n      if (type === 'visit') {\n        this.$router.push({\n          path: '/teachers/teacherDetails',\n          query: {teacherId: id}\n        })\n      } else {\n        this.$router.push({\n          path: '/teachers/teacherOperation',\n          query: {\n            type,\n            id\n          }\n        })\n      }\n    },\n    deleteData(id) {\n      this.$confirm('此操作将永久删除该老师, 是否继续?', '删除教师', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        this.$message({\n          type: 'success',\n          message: '删除成功!'\n        });\n      }).catch(() => {\n      });\n    },\n    getTeacherType() {\n      selectTypeList('teacherType')\n      .then(res => {\n        if (res.errorCode === 200) {\n          this.teacherList = res.data;\n        }\n      })\n    },\n    getGradeType() {\n      selectTypeList('grade')\n      .then(res => {\n        if (res.errorCode === 200) {\n          this.gradeList = res.data;\n        }\n      })\n    },\n    getData(params) {\n      getTeacherList(params)\n      .then(res => {\n        if (res.errorCode === 200) {\n          let data = res.data;\n          this.list = data.result;\n          console.log(res);\n          this.params.total = data.pageResult.total || 0;\n        }\n      })\n    },\n    handleCurrentChange(val) {\n      this.params.page = val;\n      this.getData(this.params);\n    },\n    handleSizeChange(val) {\n      this.params.size = val;\n      this.getData(this.params);\n    }\n  }\n}\n</script>\n\n<style scoped lang=\"scss\">\n.tip {\n  margin-right: 10px;\n  height: 22px;\n  font-size: 14px;\n  font-family: PingFangSC-Regular, PingFang SC;\n  font-weight: 400;\n  color: #2E415B;\n  line-height: 22px;\n}\n</style>"],"sourceRoot":"src/views/teachers"}]}