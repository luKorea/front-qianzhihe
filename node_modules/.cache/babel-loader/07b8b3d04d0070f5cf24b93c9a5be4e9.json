rc=\"item\"\n              lazy\n              :preview-src-list=\"info.images\"\n              fit=\"fit\"/>\n        </div>\n      </div>\n      <el-divider/>\n    </template>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"basicInfo\",\n  props: {\n    info: {\n      type: {\n        type: Object,\n        default: () => {}\n      }\n    }\n  },\n  data() {\n    return {\n      charts: ''\n    }\n  },\n  watch: {\n    info: {\n      deep: true,\n      handler() {\n        this.draw()\n      }\n    }\n  },\n  mounted() {\n    this.$nextTick(() => {\n      this.draw();\n    })\n  },\n\n  methods: {\n    draw() {\n      this.charts = this.$echarts.init(document.getElementById('sex-charts'));\n      let options = {\n        legend: {\n          top: 'center',\n          right: 'center'\n        },\n        series: [\n          {\n            type: 'pie',\n            radius: ['50%', '70%'],\n            avoidLabelOverlap: false,\n            labelLine: {show: false},\n            label: {show: false},\n            data: [\n              {\n                value: this.info.man_ratio,\n                name: `男生${this.info.man_ratio}%`,\n                itemStyle: {color: '#4D97FF'}\n              },\n              {\n                value: this.info.girl_ratio,\n                name: `女生${this.info.girl_ratio}%`,\n                itemStyle: {color: '#FC9131'}\n              }\n            ]\n          }\n        ]\n      };\n      this.charts && this.charts.setOption(options);\n    }\n  }\n}\n</script>\n\n<style scoped lang=\"scss\">\n\n.img-wrap {\n  margin-top: 10px;\n  display: flex;\n  flex-wrap: wrap;\n  justify-content: space-between;\n}\n</style>"],"sourceRoot":"src/views/universities/universitiesDetails/components"}]}