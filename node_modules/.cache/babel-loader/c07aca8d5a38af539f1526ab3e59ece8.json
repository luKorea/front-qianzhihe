AAA,EAAA;AAAA,SADA,4BAEA,KAAA,YAAA,CAAA,MAAA,EAAA,YAAA,EAAA,KAAA,EAAA;AAAA,UAAA,IAAA,EAAA;AAAA,SAAA,CAFA,sBAGA,KAAA,YAAA,CAAA,MAAA,EAAA,sBAAA,EAAA,KAAA,EAAA;AAAA,UAAA,IAAA,EAAA;AAAA,SAAA,CAHA,sBAIA,KAAA,YAAA,CAAA,MAAA,EAAA,YAAA,EAAA,KAAA,EAAA,EAAA,CAJA,sBAKA,KAAA,eAAA,CAAA,MAAA,EAAA,YAAA,EAAA,KAAA,EAAA,EAAA,CALA;AAPA,SAcA,KAAA,SAAA,8BACA,KAAA,qBAAA,CAAA,cAAA,EAAA,EAAA,CADA,sBAEA,KAAA,qBAAA,CAAA,YAAA,EAAA,EAAA,CAFA,sBAGA,KAAA,qBAAA,CAAA,YAAA,EAAA,EAAA,CAHA,GAdA;AAZA,KAAA;AAiCA,GA/CA;AAgDA,EAAA,OAhDA,qBAgDA;AACA,QAAA,KAAA,OAAA,IAAA,KAAA,OAAA,CAAA,eAAA,EAAA;AACA,WAAA,MAAA,CAAA,IAAA,GAAA,KAAA,OAAA,CAAA,eAAA;AACA;AACA,GApDA;AAqDA,EAAA,OAAA,EAAA;AACA,IAAA,UADA,wBACA;AACA,WAAA,MAAA,CAAA,KAAA,GAAA,KAAA,GAAA,CAAA,SAAA;AACA;AAHA;AArDA,CAAA","sourcesContent":["<template>\n    <div class=\"app-container\">\n        <avue-crud v-bind=\"bindVal\" v-on=\"onEvent\" v-model=\"form\" :page.sync=\"page\">\n            <template slot-scope=\"scope\" slot=\"menu\">\n                <el-button type=\"text\" size=\"small\"\n                    @click=\"rowView({ ...scope.row, formslot:'DetailsMonth',viewTitle:`商家[${row.companyName}](${dayjs(scope.row.statisticsTime).format('YYYY-MM')})的每日详情` },scope.index)\">每日详情\n                </el-button>\n            </template>\n            <div slot=\"DetailsMonthForm\" slot-scope=\"scope\">\n                <DetailsMonth :Pconfig=\"Pconfig\" :row=\"{...scope.row,...row}\" v-if=\"form&&form.formslot==scope.column.prop\"></DetailsMonth>\n            </div>\n            <template slot-scope=\"scope\" slot=\"expressFee\">\n                <span v-if=\"scope.row.expressFee&&scope.row.expressFee>0\"\n                    @click=\"rowView({ ...scope.row, formslot:'expressDay',viewTitle:`商家[${row.companyName}](${dayjs(scope.row.statisticsTime).format('YYYY-MM')})的快递详情`},scope.index)\"\n                    style=\"color: rgb(190, 149, 74);cursor: pointer;\">{{scope.row.expressFee|filtersFormatMoney}}</span>\n                <span v-else>--</span>\n            </template>\n            <template slot-scope=\"scope\" slot=\"qualityInspectionFee\">\n                <span v-if=\"scope.row.qualityInspectionFee&&scope.row.qualityInspectionFee>0\"\n                    @click=\"rowView({ ...scope.row, formslot:'qualityDay' ,viewTitle:`商家[${row.companyName}](${dayjs(scope.row.statisticsTime).format('YYYY-MM')})的质检详情`},scope.index)\"\n                    style=\"color: rgb(190, 149, 74);cursor: pointer;\">{{scope.row.qualityInspectionFee|filtersFormatMoney}}</span>\n                <span v-else>--</span>\n            </template>\n            <div slot=\"qualityDayForm\" slot-scope=\"scope\">\n                <QualityDay :Pconfig=\"Pconfig\" :row=\"{...scope.row,...row}\" v-if=\"form&&form.formslot==scope.column.prop\"></QualityDay>\n            </div>\n            <div slot=\"expressDayForm\" slot-scope=\"scope\">\n                <ExpressDay :Pconfig=\"Pconfig\" :row=\"{...scope.row,...row}\" v-if=\"form&&form.formslot==scope.column.prop\"></ExpressDay>\n            </div>\n        </avue-crud>\n    </div>\n</template>\n<script>\nimport crudMix from \"@/mixins/crudMix\";\n\nimport DetailsMonth from '@/views/company/list/UsageRecord/DetailsMonth'\nimport QualityDay from '@/views/company/list/UsageRecord/QualityDay'\nimport ExpressDay from '@/views/company/list/UsageRecord/ExpressDay'\n/**\n * TODO:使用详情\n */\nexport default {\n  components: {\n    DetailsMonth,\n    QualityDay,\n    ExpressDay\n  },\n  mixins: [\n    crudMix,\n  ],\n  props: {\n    row: {},\n    Pconfig: {},\n  },\n  data() {\n    return {\n      config: {\n        detail: '',\n        save: '',\n        delete: '',\n        update: '',\n        list: '/logCompany/orderStatistics'\n      },\n      method: {\n        list: 'GET'\n      },\n      rowKey: 'bicId',\n      option: {\n        \n        addBtn: false,\n        delBtn: false,\n        editBtn: false,\n        viewBtn: false,\n\n        column: [\n          { label: \"使用月份\", prop: \"statisticsTime\", type: \"datetime\", format: 'yyyy-MM', },\n          ...this.column_money(\"快递费用\", \"expressFee\", false, { solt: true, }),\n          ...this.column_money(\"质检费用\", \"qualityInspectionFee\", false, { solt: true, }),\n          ...this.column_money(\"使用金额\", \"monthMoney\", false, {}),\n          ...this.column_datetime(\"更新时间\", \"updateTime\", false, {}),\n        ],\n        ...this.group_def([\n          ...this.group_column_formslot(\"DetailsMonth\", {}),\n          ...this.group_column_formslot(\"qualityDay\", {}),\n          ...this.group_column_formslot(\"expressDay\", {}),\n        ]),\n      },\n    }\n  },\n  created() {\n    if (this.Pconfig && this.Pconfig.orderStatistics) {\n      this.config.list = this.Pconfig.orderStatistics;\n    }\n  },\n  methods: {\n    listBefore() {\n      this.params.bicId = this.row.companyId\n    }\n  }\n}\n</script>\n"],"sourceRoot":"src/views/company/list/UsageRecord"}]}