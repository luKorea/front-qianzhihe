,MAAA,CAAA,KAAA,CAAA,SAAA;AACA,SAAA,MAAA,CAAA,SAAA,GAAA,SAAA;AACA,SAAA,OAAA,CAAA,KAAA,MAAA;AACA,GAlBA;AAmBA,EAAA,OAAA,EAAA;AACA,IAAA,MADA,oBACA;AACA,WAAA,OAAA,CAAA,EAAA,CAAA,CAAA,CAAA;AACA,KAHA;AAIA,IAAA,OAJA,mBAIA,MAJA,EAIA;AAAA;;AACA,MAAA,cAAA,CAAA,MAAA,CAAA,CACA,IADA,CACA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,SAAA,KAAA,GAAA,EAAA;AACA,cAAA,IAAA,GAAA,GAAA,CAAA,IAAA;AACA,UAAA,KAAA,CAAA,IAAA,GAAA,IAAA,CAAA,MAAA;AACA,UAAA,KAAA,CAAA,IAAA,GAAA,IAAA,CAAA,MAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,IAAA;AACA,UAAA,KAAA,CAAA,MAAA,CAAA,KAAA,GAAA,IAAA,CAAA,UAAA,CAAA,KAAA,IAAA,CAAA;AACA;AACA,OATA;AAUA,KAfA;AAgBA,IAAA,eAhBA,2BAgBA,IAhBA,EAgBA,EAhBA,EAgBA;AACA,UAAA,IAAA,KAAA,OAAA,EAAA;AACA,aAAA,OAAA,CAAA,IAAA,CAAA;AACA,UAAA,IAAA,EAAA,qBADA;AAEA,UAAA,KAAA,EAAA;AACA,YAAA,EAAA,EAAA;AADA;AAFA,SAAA;AAMA,OAPA,MAOA;AACA,aAAA,OAAA,CAAA,IAAA,CAAA;AACA,UAAA,IAAA,EAAA,uBADA;AAEA,UAAA,KAAA,EAAA;AACA,YAAA,IAAA,EAAA,IADA;AAEA,YAAA,EAAA,EAAA;AAFA;AAFA,SAAA;AAOA;AACA,KAjCA;AAkCA,IAAA,mBAlCA,+BAkCA,GAlCA,EAkCA;AACA,WAAA,MAAA,CAAA,IAAA,GAAA,GAAA;AACA,WAAA,OAAA,CAAA,KAAA,MAAA;AACA,KArCA;AAsCA,IAAA,gBAtCA,4BAsCA,GAtCA,EAsCA;AACA,WAAA,MAAA,CAAA,IAAA,GAAA,GAAA;AACA,WAAA,OAAA,CAAA,KAAA,MAAA;AACA;AAzCA;AAnBA,CAAA","sourcesContent":["<template>\n  <div>\n    <basic-container-back>\n      <span class=\"tip-info\"></span>\n      <span class=\"tip-title\">基本信息</span>\n      <div style=\"margin-top: 20px\">\n        <el-row :gutter=\"4\" class=\"m-bottom\">\n          <el-col :span=\"10\">\n            <span class=\"teacher-title\">教师名称：</span>\n            <span class=\"teacher-info\">{{ info.name }}</span>\n          </el-col>\n          <el-col :span=\"10\">\n            <span class=\"teacher-title\">教师类型：</span>\n            <span class=\"teacher-info\">{{ info.teacherType }}</span>\n          </el-col>\n        </el-row>\n        <el-row :gutter=\"4\">\n          <el-col :span=\"10\">\n            <span class=\"teacher-title\">手机号码：</span>\n            <span class=\"teacher-info\">{{ info.phone }}</span>\n          </el-col>\n          <el-col :span=\"10\">\n            <span class=\"teacher-title\">备注：</span>\n            <span class=\"teacher-info\">{{ info.comment }}</span>\n          </el-col>\n        </el-row>\n      </div>\n    </basic-container-back>\n    <basic-container>\n      <span class=\"tip-info\"></span>\n      <span class=\"tip-title\">账号信息</span>\n      <div style=\"margin-top: 20px\">\n        <el-row :gutter=\"4\" class=\"m-bottom\">\n          <el-col :span=\"10\">\n            <span class=\"teacher-title\">登录账号：</span>\n            <span class=\"teacher-info\">{{ info.username }}</span>\n          </el-col>\n          <el-col :span=\"10\">\n            <span class=\"teacher-title\">登录密码：</span>\n            <span class=\"teacher-info\">************</span>\n          </el-col>\n        </el-row>\n      </div>\n    </basic-container>\n    <basic-container style=\"margin-bottom: 100px\">\n      <span class=\"tip-info\"></span>\n      <span class=\"tip-title\">代课班级列表</span>\n      <template v-if=\"list && list.length > 0\">\n        <el-table :data=\"list\" border style=\"width: 100%;margin: 20px 0\">\n          <el-table-column prop=\"_id\" label=\"班级ID\" align=\"center\" width=\"300\"/>\n          <el-table-column prop=\"name\" label=\"班级名称\" align=\"center\"/>\n          <el-table-column prop=\"gradeType\" label=\"班级类型\" align=\"center\"/>\n          <el-table-column prop=\"grade\" label=\"年级\" align=\"center\"/>\n          <el-table-column prop=\"enrollmentYear\" label=\"入学年份\" align=\"center\"/>\n          <el-table-column prop=\"teacherName\" label=\"班主任\" align=\"center\"/>\n          <el-table-column prop=\"teacher2Name\" label=\"生涯导师1\" align=\"center\"/>\n          <el-table-column prop=\"teacher1Name\" label=\"生涯导师2\" align=\"center\"/>\n          <el-table-column label=\"操作\" align=\"center\">\n            <template slot-scope=\"scope\">\n              <el-button type=\"text\" size=\"small\" @click=\"goOperationType('visit', scope.row._id)\">查看班级详情</el-button>\n              <el-button type=\"text\" size=\"small\" @click=\"goOperationType('edit', scope.row._id)\">编辑</el-button>\n            </template>\n          </el-table-column>\n        </el-table>\n        <basic-pagination\n            :total=\"params.total\"\n            @handleCurrentChange=\"handleCurrentChange\"\n            @handleSizeChange=\"handleSizeChange\"\n        />\n      </template>\n      <basic-nothing v-else title=\"教师绑定班级需要在 班级管理-新增/编辑班级处添加\"></basic-nothing>\n    </basic-container>\n    <div class=\"footer-btn\">\n      <el-button style=\"color: #475B75\" @click=\"goBack\">返回</el-button>\n    </div>\n  </div>\n</template>\n\n<script>\nimport {getTeacherInfo} from \"../../../api/taecher\";\n\nexport default {\n  name: \"visit\",\n  data() {\n    return {\n      params: {\n        page: 0,\n        size: 10,\n        total: 0,\n        teacherId: ''\n      },\n      info: {},\n      list: []\n    }\n  },\n  mounted() {\n    let {teacherId} = this.$route.query;\n    this.params.teacherId = teacherId;\n    this.getInfo(this.params);\n  },\n  methods: {\n    goBack() {\n      this.$router.go(-1);\n    },\n    getInfo(params) {\n      getTeacherInfo(params)\n          .then(res => {\n            if (res.errorCode === 200) {\n              let data = res.data;\n              this.info = data.object;\n              this.list = data.result;\n              console.log(this.info);\n              this.params.total = data.pageResult.total || 0;\n            }\n          })\n    },\n    goOperationType(type, id) {\n      if (type === 'visit') {\n        this.$router.push({\n          path: '/grade/gradeDetails',\n          query: {\n            id: id\n          }\n        })\n      } else {\n        this.$router.push({\n          path: '/grade/gradeOperation',\n          query: {\n            type,\n            id\n          }\n        })\n      }\n    },\n    handleCurrentChange(val) {\n      this.params.page = val;\n      this.getInfo(this.params);\n    },\n    handleSizeChange(val) {\n      this.params.size = val;\n      this.getInfo(this.params);\n    }\n  }\n}\n</script>\n\n<style scoped lang=\"scss\">\n.teacher-title {\n  height: 22px;\n  font-size: 14px;\n  font-family: PingFangSC-Regular, PingFang SC;\n  font-weight: 400;\n  color: #2E415B;\n  line-height: 22px;\n  margin-right: 20px;\n  margin-bottom: 20px;\n}\n\n.teacher-info {\n  height: 22px;\n  font-size: 14px;\n  font-family: PingFangSC-Regular, PingFang SC;\n  font-weight: 400;\n  color: #506889;\n  line-height: 22px;\n}\n</style>"],"sourceRoot":"src/views/teachers/operation"}]}