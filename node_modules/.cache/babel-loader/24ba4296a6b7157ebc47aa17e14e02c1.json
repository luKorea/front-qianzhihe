sicInfo(id)\n      .then(res => {\n        if (res.errorCode === 200) {\n          console.log(res);\n          this.info = res.data;\n        }\n      })\n    },\n    getDesc(id) {\n      getBasicDesc(id)\n      .then(res => {\n        if (res.errorCode === 200) {\n          console.log(res);\n          this.descInfo = res.data;\n        }\n      })\n    },\n    getProposal(id) {\n      getBasicProposal(id)\n      .then(res => {\n        if (res.errorCode === 200) {\n          console.log(res);\n          this.proposalInfo = res.data;\n        }\n      })\n    },\n    getEmployment(id) {\n      getBasicEmployment(id)\n      .then(res => {\n        if (res.errorCode === 200) {\n          console.log(res);\n          this.employmentInfo = res.data;\n        }\n      })\n    },\n  }\n}\n</script>\n\n<style scoped>\n\n</style>"],"sourceRoot":"src/views/major/majorDetails"}]}