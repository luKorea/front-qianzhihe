ACA,aAAA,OAAA,CAAA,IAAA,CAAA;AACA,UAAA,IAAA,EAAA,4BADA;AAEA,UAAA,KAAA,EAAA;AACA,YAAA,IAAA,EAAA,IADA;AAEA,YAAA,SAAA,EAAA,SAFA;AAGA,YAAA,OAAA,EAAA,OAAA,GAAA,OAAA,GAAA;AAHA;AAFA,SAAA;AAQA;AACA,KApBA;AAqBA,IAAA,QArBA,sBAqBA;AAAA;;AACA,MAAA,cAAA,CAAA,OAAA,CAAA,CACA,IADA,CACA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,SAAA,KAAA,GAAA,EAAA;AACA,UAAA,KAAA,CAAA,SAAA,GAAA,GAAA,CAAA,IAAA;AACA;AACA,OALA;AAMA,KA5BA;AA6BA,IAAA,YA7BA,0BA6BA;AAAA;;AACA,MAAA,cAAA,CAAA,WAAA,CAAA,CACA,IADA,CACA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,SAAA,KAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,aAAA,GAAA,GAAA,CAAA,IAAA;AACA;AACA,OALA;AAMA,KApCA;AAqCA,IAAA,OArCA,mBAqCA,MArCA,EAqCA;AAAA;;AACA,MAAA,cAAA,CAAA,MAAA,CAAA,CACA,IADA,CACA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,SAAA,KAAA,GAAA,EAAA;AACA,cAAA,IAAA,GAAA,GAAA,CAAA,IAAA;AACA,UAAA,MAAA,CAAA,IAAA,GAAA,IAAA,CAAA,MAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,UAAA,MAAA,CAAA,MAAA,CAAA,KAAA,GAAA,IAAA,CAAA,UAAA,CAAA,KAAA,IAAA,CAAA;AACA;AACA,OARA;AASA,KA/CA;AAgDA,IAAA,mBAhDA,+BAgDA,GAhDA,EAgDA;AACA,WAAA,MAAA,CAAA,IAAA,GAAA,GAAA;AACA,WAAA,OAAA,CAAA,KAAA,MAAA;AACA,KAnDA;AAoDA,IAAA,gBApDA,4BAoDA,GApDA,EAoDA;AACA,WAAA,MAAA,CAAA,IAAA,GAAA,GAAA;AACA,WAAA,OAAA,CAAA,KAAA,MAAA;AACA;AAvDA;AAvBA,CAAA","sourcesContent":["<template>\n  <div>\n    <basic-container>\n      <span class=\"tip-info\"></span>\n      <span class=\"tip-title\">学生筛选</span>\n      <div style=\"margin-top: 20px\">\n        <el-row>\n          <el-col :span=\"6\">\n            <span class=\"tip\">班级类型:</span>\n            <el-select v-model=\"params.gradeType\" placeholder=\"请选择\" clearable>\n              <template v-if=\"gradeTypeList && gradeTypeList.length > 0\">\n                <el-option v-for=\"item in gradeTypeList\" :label=\"item.name\" :value=\"item.name\"></el-option>\n              </template>\n            </el-select>\n          </el-col>\n          <el-col :span=\"6\">\n            <span class=\"tip\">年级:</span>\n            <el-select v-model=\"params.grade\" placeholder=\"请选择\" clearable>\n              <template v-if=\"gradeList && gradeList.length > 0\">\n                <el-option v-for=\"item in gradeList\" :label=\"item.name\" :value=\"item.name\"></el-option>\n              </template>\n            </el-select>\n          </el-col>\n          <el-col :span=\"6\">\n            <span class=\"tip\">班级:</span>\n            <el-select v-model=\"params.grade\" placeholder=\"请选择\" clearable>\n              <template v-if=\"gradeList && gradeList.length > 0\">\n                <el-option v-for=\"item in gradeList\" :label=\"item.name\" :value=\"item.name\"></el-option>\n              </template>\n            </el-select>\n          </el-col>\n          <el-col :span=\"6\">\n            <div style=\"display: flex\">\n              <el-input style=\"margin-right: 10px\" v-model=\"params.queryOrIdOrNameOrPhone\"\n                        placeholder=\"请输入学生ID、名称、手机号\" clearable=\"true\"/>\n              <el-button type=\"primary\" @click=\"getData(params)\">筛选</el-button>\n            </div>\n          </el-col>\n        </el-row>\n      </div>\n    </basic-container>\n    <basic-container>\n      <span class=\"tip-info\"></span>\n      <span class=\"tip-title\">学生列表</span>\n      <el-table :data=\"list\" border style=\"width: 100%;margin: 20px 0\">\n        <el-table-column prop=\"_id\" label=\"学生ID\" align=\"center\" />\n        <el-table-column label=\"头像\" align=\"center\">\n          <template slot-scope=\"scope\">\n            <el-avatar size=\"32\" :src=\"scope.row.profilePicture\"></el-avatar>\n          </template>\n        </el-table-column>\n        <el-table-column prop=\"name\" label=\"姓名\" align=\"center\" />\n        <el-table-column label=\"性别\" align=\"center\">\n          <template slot-scope=\"scope\">\n            <span>{{scope.row.gender   == 'f' ? '女' : '男'}}</span>\n          </template>\n        </el-table-column>\n        <el-table-column prop=\"username\" label=\"手机号\" align=\"center\" />\n        <el-table-column prop=\"phone\" label=\"激活日期\" align=\"center\" />\n        <el-table-column prop=\"educationLevel\" label=\"年级\" align=\"center\" />\n        <el-table-column prop=\"gradeName\" label=\"所属班级\" align=\"center\" />\n        <el-table-column prop=\"gradeType\" label=\"班级类型\" align=\"center\" />\n        <el-table-column label=\"操作\" align=\"center\">\n          <template slot-scope=\"scope\">\n            <el-button type=\"text\" size=\"small\" @click=\"goOperationType('visit', scope.row._id, scope.row.gradeId)\">查看</el-button>\n            <el-button type=\"text\" size=\"small\" @click=\"goOperationType('edit', scope.row._id, scope.row.gradeId)\">编辑</el-button>\n          </template>\n        </el-table-column>\n      </el-table>\n      <basic-pagination\n          :total=\"params.total\"\n          @handleCurrentChange=\"handleCurrentChange\"\n          @handleSizeChange=\"handleSizeChange\"\n      />\n    </basic-container>\n  </div>\n</template>\n\n<script>\nimport {getStudentList} from \"../../api/students\";\nimport {selectTypeList} from \"../../api/search\";\n\nexport default {\n  name: \"teacherList\",\n  data() {\n    return {\n      params: {\n        page: 0,\n        size: 10,\n        gradeType: '', //\t年级\n        grade: '', //班级类型\n        queryOrIdOrNameOrPhone: '',\n        total: 0\n      },\n      classList: [],\n      gradeTypeList: [],\n      gradeList: [],\n      list: []\n    }\n  },\n  mounted() {\n    this.getData(this.params);\n    this.getGradeType();\n    this.getGrade();\n  },\n  methods: {\n    goOperationType(type, studentId, gradeId) {\n      if (type === 'visit') {\n        this.$router.push({\n          path: '/students/studentDetails',\n          query: {\n            studentId: studentId,\n            gradeId: gradeId ? gradeId : ''\n          }\n        })\n      } else {\n        this.$router.push({\n          path: '/students/studentOperation',\n          query: {\n            type,\n            studentId,\n            gradeId: gradeId ? gradeId : ''\n          }\n        })\n      }\n    },\n    getGrade() {\n      selectTypeList('grade')\n      .then(res => {\n        if (res.errorCode === 200) {\n          this.gradeList = res.data;\n        }\n      })\n    },\n    getGradeType() {\n      selectTypeList('gradeType')\n      .then(res => {\n        if (res.errorCode === 200) {\n          this.gradeTypeList = res.data;\n        }\n      })\n    },\n    getData(params) {\n      getStudentList(params)\n      .then(res => {\n        if (res.errorCode === 200) {\n          let data = res.data;\n          this.list = data.result;\n          console.log(res);\n          this.params.total = data.pageResult.total || 0;\n        }\n      })\n    },\n    handleCurrentChange(val) {\n      this.params.page = val;\n      this.getData(this.params);\n    },\n    handleSizeChange(val) {\n      this.params.size = val;\n      this.getData(this.params);\n    }\n  }\n}\n</script>\n\n<style scoped lang=\"scss\">\n.tip {\n  margin-right: 10px;\n  height: 22px;\n  font-size: 14px;\n  font-family: PingFangSC-Regular, PingFang SC;\n  font-weight: 400;\n  color: #2E415B;\n  line-height: 22px;\n}\n</style>"],"sourceRoot":"src/views/students"}]}