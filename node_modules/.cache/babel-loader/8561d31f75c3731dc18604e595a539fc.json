AAA;AAFA;AAFA,OAAA;AAOA,KAdA;AAeA,IAAA,WAfA,yBAeA;AAAA;;AACA,MAAA,OAAA,GACA,IADA,CACA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,SAAA,KAAA,GAAA,EAAA;AACA,UAAA,KAAA,CAAA,QAAA,gCAAA,KAAA,CAAA,QAAA,sBAAA,GAAA,CAAA,IAAA;AACA;AACA,OALA;AAMA,KAtBA;AAuBA,IAAA,WAvBA,uBAuBA,MAvBA,EAuBA;AAAA;;AACA,MAAA,OAAA,CAAA,MAAA,CAAA,CACA,IADA,CACA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,SAAA,KAAA,GAAA,EAAA;AACA,cAAA,IAAA,GAAA,GAAA,CAAA,IAAA;AACA,UAAA,MAAA,CAAA,IAAA,GAAA,IAAA,CAAA,MAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,UAAA,MAAA,CAAA,MAAA,CAAA,KAAA,GAAA,IAAA,CAAA,UAAA,CAAA,KAAA;AACA;AACA,OARA;AASA,KAjCA;AAkCA,IAAA,gBAlCA,4BAkCA,GAlCA,EAkCA;AACA,WAAA,MAAA,CAAA,IAAA,GAAA,GAAA;AACA,WAAA,WAAA,CAAA,KAAA,MAAA;AACA,KArCA;AAsCA,IAAA,mBAtCA,+BAsCA,GAtCA,EAsCA;AACA,WAAA,MAAA,CAAA,IAAA,GAAA,GAAA;AACA,WAAA,WAAA,CAAA,KAAA,MAAA;AACA,KAzCA;AA0CA,IAAA,SA1CA,qBA0CA,EA1CA,EA0CA;AACA,WAAA,QAAA,GAAA,EAAA;AACA,WAAA,MAAA,CAAA,GAAA,GAAA,EAAA;AACA,WAAA,WAAA;AACA,WAAA,WAAA,CAAA,KAAA,MAAA;AACA,KA/CA;AAgDA,IAAA,WAhDA,yBAgDA;AACA,WAAA,MAAA,CAAA,IAAA,GAAA,EAAA;AACA,WAAA,MAAA,CAAA,IAAA,GAAA,CAAA;AACA,KAnDA;AAoDA,IAAA,SApDA,qBAoDA,YApDA,EAoDA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AACA,QAAA,IAAA,EAAA,+BADA;AAEA,QAAA,KAAA,EAAA;AACA,UAAA,YAAA,EAAA;AADA;AAFA,OAAA;AAMA;AA3DA;AA1BA,CAAA","sourcesContent":["<template>\n  <div>\n    <basic-container>\n      <div class=\"search-wrap\">\n        <el-input v-model=\"params.keywords\" placeholder=\"请输入职业名称\" clearable=\"true\"/>\n        <el-button type=\"primary\" @click=\"goSearch('occupation', params.keywords)\">搜索</el-button>\n      </div>\n      <div class=\"type-list\">\n        <span class=\"type-title\">职业分类:</span>\n        <div class='tip-wrap'>\n          <template v-if=\"menuList.length > 0\">\n          <span v-for=\"item in menuList\" :key=\"item._id\"\n                @click=\"searchTip(item._id)\"\n                :class=\"selectId === item._id ? 'select_tip tip' : 'tip'\">{{ item.name }}</span>\n          </template>\n        </div>\n      </div>\n    </basic-container>\n    <list-table\n        :list=\"list\" :params=\"params\"\n        @goDetails=\"goDetails\"\n        @handleCurrentChange=\"handleCurrentChange\"\n        @handleSizeChange=\"handleSizeChange\"\n    />\n  </div>\n</template>\n\n<script>\nimport {getMenu, getList} from '../../api/common/occupation';\nimport ListTable from './../common/occupationTable';\n\nexport default {\n  name: \"occupationList\",\n  components: {\n    ListTable\n  },\n  data() {\n    return {\n      params: {\n        keywords: '',\n        _id: '',\n        size: 10,\n        page: 0,\n        total: 0\n      },\n      list: [],\n      selectId: '',\n      menuList: [{\n        _id: '',\n        name: '不限'\n      }]\n    }\n  },\n  mounted() {\n    this.getMenuList();\n    this.getListData(this.params);\n  },\n  methods: {\n    goSearch(url, keywords) {\n      if (keywords === '') {\n        this.resetParams();\n        this.getListData(this.params);\n        return;\n      }\n      this.$router.push({\n        path: '/search/searchList',\n        query: {\n          url,\n          keywords\n        }\n      })\n    },\n    getMenuList() {\n      getMenu()\n          .then(res => {\n            if (res.errorCode === 200) {\n              this.menuList = [...this.menuList, ...res.data];\n            }\n          })\n    },\n    getListData(params) {\n      getList(params)\n          .then(res => {\n            if (res.errorCode === 200) {\n              let data = res.data;\n              this.list = data.result;\n              console.log(res);\n              this.params.total = data.pageResult.total;\n            }\n          })\n    },\n    handleSizeChange(val) {\n      this.params.size = val;\n      this.getListData(this.params);\n    },\n    handleCurrentChange(val) {\n      this.params.page = val;\n      this.getListData(this.params);\n    },\n    searchTip(id) {\n      this.selectId = id;\n      this.params._id = id;\n      this.resetParams();\n      this.getListData(this.params);\n    },\n    resetParams() {\n      this.params.size = 10;\n      this.params.page = 0;\n    },\n    goDetails(occupationId) {\n      this.$router.push({\n        path: '/occupation/occupationDetails',\n        query: {\n          occupationId: occupationId\n        }\n      })\n    }\n  }\n}\n</script>"],"sourceRoot":"src/views/occupation"}]}