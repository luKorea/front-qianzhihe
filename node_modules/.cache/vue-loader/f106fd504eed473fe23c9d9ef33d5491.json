document.getElementById('charts-one'));\n      let name = [],\n          data = [];\n      this.info.industry && this.info.industry.forEach(item => {\n        name.push(item.industry_region);\n        data.push(item.data)\n      });\n      let option = {\n        tooltip: {\n          trigger: 'axis',\n          axisPointer: {            // 坐标轴指示器，坐标轴触发有效\n            type: 'shadow'        // 默认为直线，可选为：'line' | 'shadow'\n          }\n        },\n        xAxis: {\n          type: 'category',\n          data: name,\n          axisLabel: {\n            show: true,\n            textStyle: {\n              color: '#999999',  //更改坐标轴文字颜色\n              fontSize: 13      //更改坐标轴文字大小\n            }\n          },\n        },\n        yAxis: {\n          type: 'value'\n        },\n        series: [{\n          data: data,\n          type: 'bar',\n          barMaxWidth: '40%',\n          itemStyle: {\n            normal: {\n              color(params) {\n                const colorlist = [\n                  '#FC9131', '#40A7DF', '#36E095', '#7C6AF2', '#FF6A53', '#EEA287',\n                  '#FC9131', '#40A7DF', '#36E095', '#7C6AF2', '#FF6A53', '#EEA287',\n                  '#FC9131', '#40A7DF', '#36E095', '#7C6AF2', '#FF6A53', '#EEA287',\n                ];\n                return colorlist[params.dataIndex];\n              }\n            }\n          }\n        }]\n      };\n      this.chartsOne && this.chartsOne.setOption(option);\n    },\n    drawPageTwo() {\n      this.chartsTwo = this.$echarts.init(document.getElementById('charts-two'));\n      let name = [],\n          data = [];\n      this.info.region && this.info.region.forEach(item => {\n        name.push(item.industry_region);\n        data.push(item.data)\n      });\n      let option = {\n        tooltip: {\n          trigger: 'axis',\n          axisPointer: {            // 坐标轴指示器，坐标轴触发有效\n            type: 'shadow'        // 默认为直线，可选为：'line' | 'shadow'\n          }\n        },\n        xAxis: {\n          type: 'category',\n          data: name,\n          axisLabel: {\n            show: true,\n            textStyle: {\n              color: '#999999',  //更改坐标轴文字颜色\n              fontSize: 13      //更改坐标轴文字大小\n            }\n          },\n        },\n        yAxis: {\n          type: 'value'\n        },\n        series: [{\n          data: data,\n          type: 'bar',\n          barMaxWidth: '40%',\n          itemStyle: {\n            normal: {\n              color(params) {\n                const colorlist = [\n                  '#FC9131', '#40A7DF', '#36E095', '#7C6AF2', '#FF6A53', '#EEA287',\n                  '#FC9131', '#40A7DF', '#36E095', '#7C6AF2', '#FF6A53', '#EEA287',\n                  '#FC9131', '#40A7DF', '#36E095', '#7C6AF2', '#FF6A53', '#EEA287',\n                ];\n                return colorlist[params.dataIndex];\n              }\n            }\n          }\n        }]\n      };\n      this.chartsTwo && this.chartsTwo.setOption(option);\n    }\n  }\n}\n</script>\n\n<style scoped lang=\"scss\">\n.wrap {\n  display: flex;\n  justify-content: space-between;\n  margin: 20px 0;\n  width: 100%;\n  .major-wrap {\n    display: flex;\n    //justify-content: space-evenly;\n    flex-wrap: wrap;\n    width: 100%;\n    .major-list {\n      display: flex;\n      width: 360px;\n      margin: 0 20px 20px 0;\n      flex-wrap: wrap;\n      background: #FFFFFF;\n      box-shadow: 0px 4px 16px 0px rgba(127, 137, 156, 0.3);\n      border-radius: 6px;\n      padding: 10px;\n      .img-wrap {\n        width: 170px;\n        height: 96px;\n        margin-right: 16px;\n        img {\n          width: 100%;\n          height: 100%;\n        }\n      }\n      .major-info {\n        .major-title {\n          height: 24px;\n          line-height: 24px;\n          font-size: 16px;\n          font-family: SourceHanSansSC-Medium, SourceHanSansSC;\n          font-weight: 500;\n          color: #333333;\n          //margin-bottom: 5px;\n        }\n        .major-business, .major-money {