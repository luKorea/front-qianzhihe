  .then(res => {\n            if (res.errorCode === 200) {\n              this.furtherInfo = res.data;\n            }\n          })\n    },\n    getSelectData(academyName) {\n      getSelectUniversities(academyName)\n      .then(res => {\n        if (res.errorCode === 200) {\n          this.selectInfo = res.data;\n        }\n      })\n    }\n  }\n}\n</script>\n\n<style scoped lang=\"scss\">\n</style>"]}]}